{
  "expo": {
    "name": "Jesse Krim - greatPlaces",
    "description": "Save your favorite locations! Application to practice native device features!",
    "slug": "great-places",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/location.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#171717"
    },
    "updates": {
      "fallbackToCacheTimeout": 3000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.jessekrim.greatPlaces",
      "buildNumber": "1.0.1",
      "supportsTablet": true
    },
    "android": {
      "package": "com.jessekrim.greatPlaces",
      "versionCode": 2,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB-nbtovyZYqXwoEBRQcYySxcoqVBsZ0NA"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/location.png",
        "backgroundColor": "#171717"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
